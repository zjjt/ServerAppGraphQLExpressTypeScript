        "nodemon": "^1.18.4",
        "start-server-and-test": "^1.7.1",
        "ts-jest": "^23.1.4",
        "ts-node": "^7",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.0.3"
    },
    "dependencies": {
        "apollo-server-express": "^2.0.6",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.4",
        "express": "^4.16.3",
        "graphql": "^14.0.2",
        "graphql-import": "^0.6.0",
        "graphql-request": "^1.8.2",
        "ioredis": "^4.0.0",
        "node-fetch": "^2.2.0",
